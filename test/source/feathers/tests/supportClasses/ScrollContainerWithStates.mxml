<?xml version="1.0" encoding="utf-8"?>
<f:ScrollContainer xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:f="library://ns.feathersui.com/mxml"
	name="default_property" name.setPropertyState="set_property"
	removed.setEventHandlerState="trace(event)">
	<f:states>
		<f:State name="default"/>
		<f:State name="setPropertyState"/>
		<f:State name="setEventHandlerState"/>
		<f:State name="includeInState"/>
		<f:State name="excludeFromState"/>
	</f:states>
	<f:Button id="includeInChild" label="Include In" includeIn="includeInState"/>
	<f:Button id="excludeFromChild" label="Exclude From" excludeFrom="excludeFromState"/>
</f:ScrollContainer>